@using System.Globalization
@using AddResourcessF.Carrier
@using AddResourcessF.Settings
@model SettingDTOs

<div class="card">
    <div class="card-body">
        <h4 class="card-title">@Seting.Setings</h4>
        <p class="card-description">@Seting.DataTableTitle</p>
        <form class="forms-sample"
              method="post" enctype="multipart/form-data"
              asp-controller="Setting" asp-action="Save" role="form">

            <input asp-for="Id" type="hidden" />


            <div class="form-group">
                <label for="exampleInputName1">@Seting.KiloMeterRate</label>
                <input asp-for="KiloMeterRate" type="text" class="form-control" id="exampleInputName1" placeholder="@Seting.KiloMeterRate">
                <span asp-validation-for="KiloMeterRate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label for="exampleInputName1">@Seting.KilooGramRate</label>
                <input asp-for="KilooGramRate" type="text" class="form-control" id="exampleInputName1" placeholder="@Seting.KilooGramRate">
                <span asp-validation-for="KilooGramRate" class="text-danger"></span>
            </div>
         

      

            <button type="submit" formmethod="post" class="btn btn-gradient-primary me-2">@Seting.Save</button>
            <button class="btn btn-light" asp-controller="Setting" asp-action="List">@Seting.Cansel</button>
        </form>
    </div>
</div>





